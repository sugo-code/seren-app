{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "open-iconic@1.1.1",
      "destination": "wwwroot/lib/open-iconic/",
      "files": [
        "font/fonts/open-iconic.eot",
        "font/css/open-iconic-bootstrap.min.css",
        "font/fonts/open-iconic.otf",
        "font/fonts/open-iconic.svg",
        "font/fonts/open-iconic.ttf",
        "font/fonts/open-iconic.woff"
      ]
    },
    {
      "library": "Chart.js@3.8.0",
      "destination": "wwwroot/lib/chart.js",
      "files": [
        "chart.min.js"
      ]
    },
    {
      "library": "date-fns@1.30.1",
      "destination": "wwwroot/lib/date-fns/",
      "files": [
        "date_fns.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "chartjs-adapter-date-fns@2.0.0",
      "destination": "wwwroot/lib/chartjs-adapter-date-fns/",
      "files": [
        "dist/chartjs-adapter-date-fns.bundle.min.js"
      ]
    }
  ]
}